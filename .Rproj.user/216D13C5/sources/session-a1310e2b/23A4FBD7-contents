#######################
##  R for bigenners  ##
#######################

age <- c(23,67,45)
class(age)
length(age)

# run == ctrl + enter 

age2 <- age + 5 
age2

age3<- c(5,5,5)

age4 <- age * age3
age4


sex <- c("F","M","F")
class(sex)

typeof(sex)


df <- data.frame(age,sex)
df

View(df)

id <- c("1","2","3")

df2 <- cbind(id,df)
df2